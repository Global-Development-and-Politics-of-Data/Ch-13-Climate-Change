@article{Walker2020a,
  title = {The Digital Life of the \#migrantcaravan: {{Contextualizing Twitter}} as a Spatial Technology},
  shorttitle = {The Digital Life of the \#migrantcaravan},
  author = {Walker, Margath A and Frimpong Boamah, Emmanuel},
  date = {2020-07-01},
  journaltitle = {Big Data \& Society},
  shortjournal = {Big Data \& Society},
  volume = {7},
  pages = {2053951720978485},
  publisher = {{SAGE Publications Ltd}},
  issn = {2053-9517},
  doi = {10/gh6jbq},
  abstract = {The Central American migrant caravans of 2018 are best understood as having been precipitated by entangled multi-scalar geopolitical histories among the United States, Mexico, Guatemala, Honduras, and El Salvador. Unsurprisingly, the migrants traveling north to the United States garnered widespread attention on social media. So much so that the reaction to the caravan accelerated plans to deploy troops to the US southern border and deny Central Americans the opportunity to seek asylum. This example showcases how the digital world can have exponential material effects. While coverage on border security and migration has been extensive, within political geography, such concerns have rarely been paired with social media. In this article, we take as our object of analysis the digitality or â€œdigital lifeâ€ of the migrant caravan. Mapping the patterns of migrant caravan-related tweeting paired with the exploration of Twitterâ€™s networked dimensions reveals the platform to be a fundamentally spatial technology. Rather than reflect, refract or distort, Twitter produces and (its power) is in turn produced through spatial mechanisms. We present multiple cartographic visualizations in support of this claim and highlight the ways in which a contextual knowledge of the subject under studyâ€”the migrant caravanâ€”can further inform analyses of Big Data.},
  annotation = {00000},
  keywords = {Big Data,borders,geopolitics,migration,mixed methods,Twitter},
  langid = {english},
  number = {2}
}

