  author = {RÃ¤sÃ¤nen, Aleksi and Juhola, Sirkku and Nygren, Anja and KÃ¤kÃ¶nen, Mira and Kallio, Maarit and Monge Monge, AdriÃ¡n and Kanninen, Markku},
