  abstract = {Since 2014 the (relatively) calm waters of the EUâ€™s trade policy have been roiled by wide-spread popular opposition to the EUâ€™s trade negotiations with the United States and Canada and the apparent spread of anti-globalization populism. The Commissionâ€™s â€˜balanced and progressiveâ€™ trade strategy is a response to these developments. This article assesses whether the response is adequate. It concludes that the strategy is unlikely to address the identified problem, because it largely reflects continuity with past practices that did not prevent or resolve the politicization of trade policy. It also concludes that the Commissionâ€™s assessment of the politicization of trade policy is exaggerated. Thus, two wrongs may have produced the â€˜rightâ€™ policy, at least in the narrow sense that EU trade policy is unlikely to be as politically fraught in future.},
  annotation = {00016},
  keywords = {European Union,international trade,politicization,trade policy},
  number = {12}
}

