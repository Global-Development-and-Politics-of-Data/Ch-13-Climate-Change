  author = {MartÃ­, Luisa and Puertas, Rosa and GarcÃ­a, Leandro},
  date = {2014-08-23},
  journaltitle = {Applied Economics},
  volume = {46},
  pages = {2982--2992},
  issn = {0003-6846},
  doi = {10.1080/00036846.2014.916394},
  abstract = {Logistics and transport increasingly play a pivotal role in international trade relations. The Logistics Performance Index (LPI) analyses differences between countries in terms of customs procedures, logistics costs and the quality of the infrastructure for overland and maritime transport. The aim of this article is to analyse the impact that each of these components has on trade in emerging economies using a gravity model. Furthermore, the study also attempts to detect possible advances in logistics in developing countries, which are grouped into five regions (Africa, South America, Far East, Middle East and Eastern Europe) by comparing the first LPI data published in 2007 with the most recent data, released in 2012. The results obtained reveal that improvements in any of the components of the LPI can lead to significant growth in a countryâ€™s trade flows. Specifically, LPI components are becoming increasingly important for international trade in many countries in Africa, South America and Eastern Europe.},
  annotation = {00143},
