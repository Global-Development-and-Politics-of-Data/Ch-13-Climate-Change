@article{Roth2020,
  title = {Global {{Burden}} of {{Cardiovascular Diseases}} and {{Risk Factors}}, 1990-2019: {{Update From}} the {{GBD}} 2019 {{Study}}},
  shorttitle = {Global {{Burden}} of {{Cardiovascular Diseases}} and {{Risk Factors}}, 1990-2019},
  author = {Roth, Gregory A. and Mensah, George A. and Johnson, Catherine O. and Addolorato, Giovanni and Ammirati, Enrico and Baddour, Larry M. and Barengo, NoÃ«l C. and Beaton, Andrea Z. and Benjamin, Emelia J. and Benziger, Catherine P. and Bonny, AimÃ© and Brauer, Michael and Brodmann, Marianne and Cahill, Thomas J. and Carapetis, Jonathan and Catapano, Alberico L. and Chugh, Sumeet S. and Cooper, Leslie T. and Coresh, Josef and Criqui, Michael and DeCleene, Nicole and Eagle, Kim A. and Emmons-Bell, Sophia and Feigin, Valery L. and FernÃ¡ndez-SolÃ , Joaquim and Fowkes, Gerry and Gakidou, Emmanuela and Grundy, Scott M. and He, Feng J. and Howard, George and Hu, Frank and Inker, Lesley and Karthikeyan, Ganesan and Kassebaum, Nicholas and Koroshetz, Walter and Lavie, Carl and Lloyd-Jones, Donald and Lu, Hong S. and Mirijello, Antonio and Temesgen, Awoke Misganaw and Mokdad, Ali and Moran, Andrew E. and Muntner, Paul and Narula, Jagat and Neal, Bruce and Ntsekhe, Mpiko and Moraes de Oliveira, Glaucia and Otto, Catherine and Owolabi, Mayowa and Pratt, Michael and Rajagopalan, Sanjay and Reitsma, Marissa and Ribeiro, Antonio Luiz P. and Rigotti, Nancy and Rodgers, Anthony and Sable, Craig and Shakil, Saate and Sliwa-Hahnle, Karen and Stark, Benjamin and SundstrÃ¶m, Johan and Timpel, Patrick and Tleyjeh, Imad M. and Valgimigli, Marco and Vos, Theo and Whelton, Paul K. and Yacoub, Magdi and Zuhlke, Liesl and Murray, Christopher and Fuster, Valentin and {GBD-NHLBI-JACC Global Burden of Cardiovascular Diseases Writing Group}},
