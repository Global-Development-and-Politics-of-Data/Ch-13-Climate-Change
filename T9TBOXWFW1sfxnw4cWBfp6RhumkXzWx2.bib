@article{Mukherjee2018,
  title = {A {{Novel Methodological Approach}} to {{Estimate}} the {{Impact}} of {{Natural Hazard}}-{{Induced Disasters}} on {{Country}}/{{Region}}-{{Level Economic Growth}}},
