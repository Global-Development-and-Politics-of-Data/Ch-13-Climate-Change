  keywords = {crisis management,emergency response,global health security,GOVERNMENT regulation,International Health Regulations,INTERNATIONAL public health laws,INTERNATIONAL relations,LEADERSHIP,MEDICAL emergencies,WORLD health,WORLD Health Organization,World Health Organization reform},
