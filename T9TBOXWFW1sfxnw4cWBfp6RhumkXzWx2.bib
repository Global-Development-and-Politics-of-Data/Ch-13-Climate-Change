@article{Beerli2017b,
  title = {The {{Power}} to {{Count}} and the {{Stakes}} of {{Counting}}: {{An Inquiry}} into the {{Quantified Production}} of {{Humanitarian Insecurity}}},
  shorttitle = {The {{Power}} to {{Count}} and the {{Stakes}} of {{Counting}}},
  author = {Beerli, Monique},
