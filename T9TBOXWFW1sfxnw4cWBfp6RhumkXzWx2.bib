@article{Teixeira2016,
  title = {Economic Growth, Human Capital and Structural Change: {{A}} Dynamic Panel Data Analysis},
  shorttitle = {Economic Growth, Human Capital and Structural Change},
  author = {Teixeira, Aurora A. C. and QueirÃ³s, Anabela S. S.},
